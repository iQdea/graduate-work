  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "docker:start": "docker-compose -p \\\"storage-stack\\\" -f diplom/docker-compose.yml up -d",
    "start": "nest start",
    "start:dev": "env-cmd yarn start --watch | pino-pretty -i pid,hostname,context -t HH:MM:ss",
    "start:debug": "yarn start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "lint:dev": "eslint \"src/**/*.ts\"",
    "lint": "yarn lint:dev --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=production  jest --config ./test/jest-e2e.json",
    "db:generate": "ts-node src/common/scripts/generate-database.ts",
    "prepare": "node -e 'if (process.env.CI === undefined) { require(\"husky\").install(); }'"
  },
